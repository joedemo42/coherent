
/box {
        x y moveto
        x 100 add y lineto
        x 100 add y 100 sub lineto
        x y 100 sub lineto
        closepath
} def

/circle1 {
        x 75 add y 25 sub 10 0 360 arc
        stroke
} def

/circle2 {
        x 25 add y 75 sub 10 0 360 arc
        fill
} def

/line {
        10 setlinewidth
        x 5 add y 5 sub moveto
        x 95 add y 95 sub lineto
        stroke
        1 setlinewidth
} def            

/letters {
        x 30 add y 65 sub moveto
        (ABC) show
} def

/Times-Roman findfont 30 scalefont setfont

/objects [
        {circle1}
        {circle2}
        {line}
        {letters}
] def

/x 100 def
/y 600 def

0 1 3 {
        /backgray exch def
        0 1 3 {
                /foregray exch def
                box stroke
                backgray 3 div setgray
                box fill
                foregray 3 div setgray
                objects {
                        exec
                } forall
                /x x 100 add def
                0 setgray
        } for
        /x 100 def
        /y y 100 sub def
} for        

showpage
