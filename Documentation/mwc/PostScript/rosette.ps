% rosette.ps 11/11/90
% PostScript Language Tutorial and Cookbook, p. 133.

/inch {72 mul} def

/wedge
{
	newpath
	0 0 moveto
	1 0 translate
	15 rotate
	0 15 sin translate
	0 0 15 sin -90 90 arc
	closepath
} def

gsave
	4.25 inch 5.5 inch translate
	3 inch 3 inch scale
	0.02 setlinewidth
	1 1 12 {
		12 div setgray
		gsave
			wedge
			gsave fill grestore
			0 setgray stroke
		grestore
		30 rotate
	} for
grestore

showpage

% end of rosette.ps
