% fontnames.ps 11/13/90
% Print the names of available fonts.

/inch {72 mul} def
/s 50 string def
/Times-Roman findfont 12 scalefont setfont
1 inch 10 inch translate

FontDirectory			% push font directory
{				% forall key/value pairs
	pop			% lose the value
	s cvs			% convert key to string
	0 0 moveto		% set current point
	show			% print the key
	0 -15 translate		% move to next line
} forall

showpage

% end of fontnames.ps
