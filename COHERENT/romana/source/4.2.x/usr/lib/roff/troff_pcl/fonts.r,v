.lf R tr100rpn.usp
.lf B tr100bpn.usp
.lf I tr100ipn.usp
.rf R TR
.rf B TB
.rf I TI\}
.\" The requests below load font tables for 18 HP LJIII internal fonts.
.\" If you are using an LJIII, change the conditionals here and above.
.if 0 \{
.lf B  CGTimes_B.fwt
.lf J  CGTimes_BI.fwt
.lf I  CGTimes_I.fwt
.lf R  CGTimes_R.fwt
.rf B  TB
.rf J  TJ
.rf I  TI
.rf R  TR
.lf MB Cour10_B.fwt
.lf MI Cour10_I.fwt
.lf MR Cour10_R.fwt
.lf LB Cour12L_B.fwt
.lf LR Cour12L_R.fwt
.lf CB Cour12_B.fwt
.lf CI Cour12_I.fwt
.lf CR Cour12_R.fwt
.lf LL LinepL_R.fwt
.lf L  Linep_R.fwt
.lf UB Univers_B.fwt
.lf UJ Univers_BI.fwt
.lf UI Univers_I.fwt
.lf UR Univers_R.fwt
.ps 10\}
.\" end of /usr/lib/roff/troff_pcl/fonts.r
