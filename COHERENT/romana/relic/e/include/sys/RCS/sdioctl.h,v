#define __SYS_SDIOCTL_H__

#define	SDIOC		('S'<<8)
#define	SCSI_HA_CMD	(SDIOC|0)
#define	SCSI_CMD	(SDIOC|1)
#define	SCSI_CMD_IN	(SDIOC|2)
#define	SCSI_CMD_OUT	(SDIOC|3)

#endif
