	.globl	blkmv

blkmv:	push	si
	push	di
	push	bp
	mov	bp, sp

	mov	di, 8(bp)
	mov	si, 10(bp)
	mov	cx, 12(bp)
	cld
	rep
	movsb

	mov	ax, 8(bp)
	mov	sp, bp
	pop	bp
	pop	di
	pop	si
	ret
@
