/*
 * ioctl commands for /dev/null.
 */

#ifndef __SYS_NULL_H__
#define __SYS_NULL_H__

#define NLIOC	('N' << 8)
#define NLCALL	(NLIOC|1)	/* Call a function with arguments.  */
#define NLIDLE	(NLIOC|2)	/* Get idle time of the system process */

#endif /* NULL_H */
@


