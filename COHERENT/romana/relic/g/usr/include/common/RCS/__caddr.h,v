@#ifndef	__COMMON___CADDR_H__
#define	__COMMON___CADDR_H__

/*
 * This internal header file is intended as the sole point of definition for
 * the internal data type "__caddr_t", exactly equivalent to the System V data
 * type "caddr_t" but given an internal name to avoid unnecessary intrusion
 * upon the user namespace.
 */

typedef	char	      *	__caddr_t;

#endif	/* ! defined (__COMMON___CADDR_H__) */

@
