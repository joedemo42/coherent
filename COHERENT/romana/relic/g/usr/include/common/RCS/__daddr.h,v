@#ifndef	__COMMON___DADDR_H__
#define	__COMMON___DADDR_H__

/*
 * This internal header file is intended as the sole point of definition for
 * the internal data type "__daddr_t", exactly equivalent to the SVR4 data
 * type "daddr_t" but given an internal name so that header files may refer
 * to the type without exporting the user-level type unnecessarily.
 */

typedef	long		__daddr_t;

#endif	/* ! defined (__COMMON___DADDR_H__) */
@
