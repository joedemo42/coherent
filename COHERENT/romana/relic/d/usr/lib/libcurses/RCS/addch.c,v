#ifndef COHERENT
#ifndef lint
static uchar sccsid[] = "@@(#)addch.c	5.4 (Berkeley) 6/30/88";
#endif /* not lint */
#endif /* not COHERENT */

# include	"curses.ext"

/*
 *	This routine adds the character to the current position
 *
 */
waddch(win, c)
WINDOW	*win;
uchar		c;
{
    return waddbytes(win, &c, 1);
}
@


