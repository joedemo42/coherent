# /conf/taylor.post 9/1/93
/bin/echo '\fWe will now install the Taylor UUCP files.'
/bin/echo '\nCopying Taylor UUCP files into place...'
cpdir -ade /tmp/tay_new_files/usr /usr
rm -r /tmp/tay_new_files
cpdir -ade /tmp/tay_config /usr/lib/uucp/
rm -r /tmp/tay_config
/bin/echo '\nUUCP configuration files are now in place.'
/bin/echo 'Hit <Enter> to continue...'
read x
/bin/echo -n '\f'
